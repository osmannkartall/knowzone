{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": ["error", { "allow": ["_id", "__v"] }],
        "object-curly-newline": "off",
        "no-param-reassign": "off"
    }
}
